#include <iostream>
#include <vector>


double getMean(){
  return 3.54;
}

std::vector<double> getResult(){

  std::vector<double> for_calc = {10, 30, getMean()};
  return for_calc;

}

int main(){
  std::vector<double> g;
  g = getResult();
  for(int i = 0; i<g.size(); i++){
    std::cout << g[i] << std::endl;
  }
}
